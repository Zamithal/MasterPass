<UserControl x:Class="MasterPass.HashedPasswordCompactDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MasterPass"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="150">
	<Grid Margin="2">
		<Border BorderBrush="Black" CornerRadius="3" Background="White" BorderThickness="2"
				Name="uxHashButton" 
				MouseLeftButtonDown="uxHashButton_MouseLeftButtonDown" 
				MouseRightButtonDown="uxHashButton_MouseRightButtonDown">
			<TextBlock Name="uxDateCreated" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="20" FontWeight="Bold">MISSING</TextBlock>
		</Border>
	</Grid>
</UserControl>
